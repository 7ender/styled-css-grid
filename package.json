{
  "name": "styled-css-grid",
  "version": "0.0.0",
  "description": "A lightweight CSS grid layout for React, built with styled-components",
  "main": "dist/index.js",
  "repository": "https://github.com/azz/styled-css-grid",
  "author": "Lucas Azzola",
  "license": "MIT",
  "files": [
    "dist/index.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --write \"lib/*.js\"",
    "test": "jest",
    "build": "yarn build:bundle && yarn build:examples",
    "build:bundle": "browserify index.js -o dist/index.js -s StyledCssGrid",
    "build:examples": "node scripts/build-examples"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "styled-components": "^2"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-standalone": "^6.25.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "jest-styled-components": "^4.2.2",
    "prettier": "^1.5.3",
    "react": "next",
    "react-dom": "next",
    "react-test-renderer": "next",
    "shelljs": "^0.7.8",
    "styled-components": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "react": "React",
    "react-dom": "ReactDOM"
  }
}
